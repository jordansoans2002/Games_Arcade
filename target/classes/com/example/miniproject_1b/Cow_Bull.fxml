<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="360" prefWidth="270.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.miniproject_1b.CowBull">

    <opaqueInsets>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </opaqueInsets>

    <TextField fx:id="enterGuesses" layoutX="15.0" layoutY="13.0" onKeyTyped="#submit" prefHeight="39.0" prefWidth="111.0" promptText="Guess" />
    <Label layoutX="141.0" layoutY="20.0" text="Cows" textFill="GREEN"/>
    <Label layoutX="203.0" layoutY="20.0" text="Bulls" textFill="RED"/>

    <ScrollPane fitToWidth="true" layoutX="5.0" layoutY="58.0" prefHeight="310.0" prefWidth="260.0" >
        <content>
            <GridPane fx:id="allGuesses" alignment="CENTER" hgap="45.0" vgap="7.0">
                <padding><Insets top="5.0" right="5.0"/></padding>
            </GridPane>
        </content>
      <padding>
         <Insets top="7" left="7.0" bottom="7" right="20.0" />
      </padding>
      <!--<rotationAxis>
         <Point3D />
      </rotationAxis>-->
    </ScrollPane>
  <!-- <rotationAxis>
      <Point3D />
   </rotationAxis>-->
</AnchorPane>
